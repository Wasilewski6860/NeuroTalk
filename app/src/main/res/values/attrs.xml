<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RegistrationTabBar">
        <attr name="android:entries"/>
    </declare-styleable>

    <attr name="registrationBarItemColor" format="color"/>

    <declare-styleable name="CustomCardView">
        <attr name="cardImage" format="reference"/>
        <attr name="cardText" format="reference"/>
        <attr name="cardBackground" format="reference"/>
    </declare-styleable>

    <declare-styleable name="MainScreenHeaderView">
        <attr name="userAvatar" format="reference"/>
        <attr name="userName" format="reference"/>
    </declare-styleable>

    <declare-styleable name="CustomTextView">
        <attr name="firstWord" format="reference"/>
        <attr name="secondWord" format="reference"/>
    </declare-styleable>
</resources>